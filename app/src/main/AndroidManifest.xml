<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bmiproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_web"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_web"
        android:supportsRtl="true"
        android:theme="@style/Theme.BMIProject">
        <activity
            android:name=".BMIResult"
            android:exported="false"
            android:parentActivityName=".BMIActivity" />
        <activity
            android:name=".resultsActivity"
            android:exported="false"
            android:label="Basal Metabolic Rate"
            android:parentActivityName=".BMRActivity" />
        <activity
            android:name=".BMRActivity"
            android:exported="false"
            android:label="BMR"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BMIActivity"
            android:label="BMI"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>